{
	"plugins": [
		"plugins/markdown",
		"node_modules/better-docs/component",
		"node_modules/better-docs/category"
	],
	"ignoreCode": {
		"dynamicImports": true
	},
	"recurseDepth": 10,
	"source": {
		"include": ["./components", "./pages", "./public", "./services", "meta/agenda.txt"],
		"includePattern": ".+\\.js(doc|x)?$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": [
			"category",
			"component"
		],
		"dictionaries": [
			"jsdoc",
			"closure"
		]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false,
		"default": {
			"staticFiles": {
				"include": [
					"./docs-src/statics"
				]
			}
		},
		"better-docs": {
			"name": "The Hub, v2.1 &mdash; Client",
			"logo": "images/BrandLogoOnly.png",
			"hideGenerator": true
		}
	},
	"opts": {
		"template": "node_modules/better-docs",
		"destination": "docs",
		"recurse": true,
		"verbose": true
	}
}